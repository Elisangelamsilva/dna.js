{
   "name": "dna.js",
   "version": "1.5.2",
   "description": "An uncomplicated user interface library for semantic templates",
   "summary": "dna.js is a lightweight easy-to-use UI library for jQuery enabling developers to rapidly build maintainable JavaScript applications.",
   "license": "MIT",
   "main": "dna.js",
   "browser": "dist/dna.min.js",
   "repository": "github:dnajs/dna.js",
   "homepage": "https://github.com/dnajs/dna.js",
   "bugs": "https://github.com/dnajs/dna.js/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "clone",
      "dom",
      "engine",
      "html",
      "jquery",
      "json",
      "template",
      "templating",
      "ui"
   ],
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "$": true,
         "dna": true,
         "window": true
      }
   },
   "scripts": {
      "web-server": "http-server -p 3482 &",
      "pretest": "jshint *.js spec/*.js website/**/*.js && gulp build-dist",
      "test": "mocha spec/*.js && env specMode=minified mocha spec/*.js",
      "posttest": "gulp report-size",
      "preweb": "gulp clean-web",
      "web": "export dnaReleasedVersion=$(git tag | tail -1) && gulp build-web",
      "postweb": "gulp other-stuff"
   },
   "peerDependencies": {
      "jquery": ">=2.2"
   },
   "devDependencies": {
      "@babel/core": "~7.2",
      "@babel/preset-env": "~7.2",
      "@fortawesome/fontawesome-free": "~5.5",
      "del": "~3.0",
      "fetch-json": "~2.2",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-babel-minify": "~0.5",
      "gulp-file-include": "~2.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~2.2",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~1.4",
      "http-server": "~0.11",
      "jquery": "~3.3",
      "jsdom": "~13.0",
      "jshint": "~2.9",
      "merge-stream": "~1.0",
      "mocha": "~5.2",
      "web-ignition": "~1.0"
   }
}
