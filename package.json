{
   "name": "dna.js",
   "version": "1.6.0",
   "description": "An uncomplicated user interface library for building data-driven semantic templates",
   "summary": "dna.js is a lightweight easy-to-use UI library enabling developers to rapidly build maintainable JavaScript applications.",
   "license": "MIT",
   "main": "dist/dna.js",
   "browser": "dist/dna.min.js",
   "style": "dist/dna.css",
   "repository": "github:dnajs/dna.js",
   "homepage": "https://github.com/dnajs/dna.js",
   "bugs": "https://github.com/dnajs/dna.js/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "clone",
      "dom",
      "engine",
      "html",
      "jamstack",
      "jquery",
      "json",
      "template",
      "templating",
      "ui"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "$": true,
         "dna": true,
         "window": true
      }
   },
   "scripts": {
      "web-server": "http-server --port 3482 &",
      "pretest": "jshint *.js spec website && gulp build-dist",
      "test": "mocha spec && env specMode=minified mocha spec",
      "posttest": "gulp report-size",
      "preweb": "gulp clean-web",
      "web": "export dnaReleasedVersion=$(git tag | tail -1) && gulp build-web",
      "postweb": "gulp other-stuff"
   },
   "peerDependencies": {
      "jquery": ">=3.4"
   },
   "devDependencies": {
      "@babel/core": "~7.7",
      "@babel/preset-env": "~7.7",
      "@fortawesome/fontawesome-free": "~5.12",
      "babel-minify": "~0.5",
      "canvas": "~2.6",
      "del": "~5.1",
      "fetch-json": "~2.2",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-file-include": "~2.1",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~3.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~1.4",
      "highlight.js": "~9.17",
      "http-server": "~0.12",
      "jquery": "~3.4",
      "jsdom": "~15.2",
      "jshint": "~2.10",
      "merge-stream": "~2.0",
      "mocha": "~6.2",
      "request": "~2.88",
      "web-ignition": "~1.1"
   }
}
